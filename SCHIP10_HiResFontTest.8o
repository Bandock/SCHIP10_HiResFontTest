# SuperCHIP V1.0 High Resolution Font Test
# By Joshua Moss (Bandock)

: main
v0 := 0
v1 := 0
v2 := 0x08
v3 := 5
vf := 0
i := hex v2
sprite v0 v1 10
v2 := 0x18
i := hex v2
sprite v0 v1 10
sprite v0 v1 10
if vf != 0 then jump schip_10_found
sprite v0 v1 5
if vf != 0 then jump chip8_found
clear
v0 := 28
v1 := 13
i := failed
sprite v0 v1 5
loop
again

: schip_10_found
clear
v0 := 14
v1 := 11
v2 := 5
i := schip_10
draw_text
v0 := 14
v1 += 6
v2 := 4
i := found
draw_text
loop
again

: chip8_found
clear
v0 := 14
v1 := 11
v2 := 4
i := chip8
draw_text
v0 := 14
v1 += 6
v2 := 4
i := found
draw_text
loop
again

: draw_text
loop
	sprite v0 v1 5
	v0 += 8
	v2 += 255
	i += v3
	while v2 != 0
again
;


